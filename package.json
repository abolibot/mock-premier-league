{
  "name": "mock-premier-league-api",
  "version": "1.0.0",
  "description": "engineering assessment",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "watch": "NODE_ENV=development webpack --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rm -rf ./dist && NODE_ENV=production webpack",
    "test-watch": "jest --watchAll --no-cache",
    "test": "jest",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/test/setup.ts"
    ],
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1"
    }
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abolibot/mock-premier-league.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "webpack",
    "pnpm"
  ],
  "author": "Oluwatobi Alaran <alarantobiloba@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abolibot/mock-premier-league/issues"
  },
  "homepage": "https://github.com/abolibot/mock-premier-league#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.5.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.15",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.2",
    "jest": "^29.7.0",
    "lint-staged": "^13.1.0",
    "mongodb-memory-server": "^8.15.1",
    "nodemon-webpack-plugin": "^4.8.1",
    "prettier": "3.0.3",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.4",
    "uglify-js": "^3.17.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "helmet": "^6.0.1",
    "moment-logger": "1.0.5-12",
    "mongoose": "^7.5.3",
    "morgan": "^1.10.0",
    "redis": "^4.6.10"
  }
}